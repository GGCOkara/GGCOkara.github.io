      "url": "/about/",
      "icons": [
        {
          "src": "./src/icon/128.png",
          "type": "image/png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Cources",
      "url": "/courses/",
      "icons": [
        {
          "src": "./src/icon/128.png",
          "type": "image/png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Developer",
      "url": "/mramzanch",
      "icons": [
        {
          "src": "./src/icon/mramzanch.png",
          "type": "image/png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Maintainor",
      "url": "/mrc/",
      "icons": [
        {
          "src": "./src/icon/mrc.png",
          "type": "image/png",
          "sizes": "192x192"
        }
